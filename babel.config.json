{
  "presets": [
    [
      "@babel/preset-env",
      {
        // "targets": "last 2 Chrome versions"
        // "targets": { "ie": "11" }
        // "targets": "> 0.25%, not dead"
        "targets": "> 0.5%, not dead",
        "useBuiltIns": "usage",
        "corejs": { "version": 3, "proposals": true }
      }
    ]
  ],
  "plugins": [
    ["@babel/plugin-proposal-pipeline-operator", { "proposal": "minimal" }]
  ]
}
